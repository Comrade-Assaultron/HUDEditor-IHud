<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<HUDEditor>
	<Elements>
		<!--
			All X and Y positions are relative to their default position on the HUD.
			X is left and right; Y is up and down.
		-->
		<!--Quickloot, annoyingly, applies to some containers rollover-->
		<QuickLoot>
			<Scale>1</Scale>
			<X>0</X>
			<Y>0</Y>
		</QuickLoot>
		<!--Frobber are interact prompts like unlicking locked doors and interacting with NPCs-->
		<Frobber>
			<Scale>1</Scale>
			<X>0</X>
			<Y>0</Y>
		</Frobber>
		<!--RollOver is essentially the same as Frobber (interact prompts)-->
		<RollOver>
			<Scale>1</Scale>
			<X>0</X>
			<Y>0</Y>
		</RollOver>
		<Compass>
			<Scale>1</Scale>
			<X>0</X>
			<Y>0</Y>
		</Compass>
		<QuestTracker>
			<Scale>1</Scale>
			<X>0</X>
			<Y>0</Y>
		</QuestTracker>
		<Notification>
			<Scale>1</Scale>
			<X>0</X>
			<Y>0</Y>
		</Notification>
		<!--Health meter-->
		<LeftMeter>
			<Scale>1</Scale>
			<X>0</X>
			<Y>0</Y>
			<!--Shows/hides the "HP" text to the left of the HP meter-->
			<ShowHPLabel>true</ShowHPLabel>
			<!--Moves the HP text to the left or right of the meter-->
			<HPLabelSide>left</HPLabelSide>
			<!--Shows/hides the HP bar background. Looks strange with ImprovedHealthBar, not a bug.-->
			<ShowBarBG>true</ShowBarBG>
		</LeftMeter>
		<!--AP meter, active effects-->
		<RightMeter>
			<Parts>
				<APMeter>
					<Scale>1</Scale>
					<X>0</X>
					<Y>0</Y>
					<!--Shows/hides the "AP" text-->
					<ShowAPLabel>true</ShowAPLabel>
					<!--Moves the AP text to the left or right of the meter-->
					<APLabelSide>right</APLabelSide>
					<!--Shows/hides the AP bar background-->
					<ShowBarBG>true</ShowBarBG>
				</APMeter>
				<ActiveEffects>
					<Scale>1</Scale>
					<X>0</X>
					<Y>0</Y>
					<PowerArmor>
						<X>0</X>
						<Y>0</Y>
					</PowerArmor>
				</ActiveEffects>
				<HungerMeter>
					<Scale>1</Scale>
					<X>0</X>
					<Y>0</Y>
					<PowerArmor>
						<Scale>1</Scale>
						<X>0</X>
						<Y>0</Y>
					</PowerArmor>
				</HungerMeter>
				<ThirstMeter>
					<Scale>1</Scale>
					<X>0</X>
					<Y>0</Y>
					<PowerArmor>
						<Scale>1</Scale>
						<X>0</X>
						<Y>0</Y>
					</PowerArmor>
				</ThirstMeter>
				<AmmoCount>
					<Scale>1</Scale>
					<X>0</X>
					<Y>0</Y>
				</AmmoCount>
				<ExplosiveAmmoCount>
					<Scale>1</Scale>
					<X>0</X>
					<Y>0</Y>
					<PowerArmor>
						<X>0</X>
						<Y>0</Y>
					</PowerArmor>
				</ExplosiveAmmoCount>	
				<FlashLightWidget>
					<!--There is no FlashLight icon in Fallout 76 by default...
						Enable this setting to restore it and enable scaling and moving it-->
					<RestoreFlashLightIcon>false</RestoreFlashLightIcon>
					<Scale>1</Scale>	
					<X>0</X>
					<Y>0</Y>
				</FlashLightWidget>
				<Emote>
					<Scale>1</Scale>
					<X>0</X>
					<Y>0</Y>
				</Emote>
			</Parts>
		</RightMeter>
		<!--Sneaking meter (hidden, danger, etc)-->
		<StealthMeter>
			<Scale>1</Scale>
			<X>0</X>
			<Y>0</Y>
		</StealthMeter>
		<!--quest/event complete/rewards list-->
		<Announce>
			<Scale>1</Scale>
			<X>0</X>
			<Y>0</Y>
		</Announce>
		<!--team mate list-->
		<TeamPanel>
			<Scale>1</Scale>
			<X>0</X>
			<Y>0</Y>
			<PowerArmor>
				<Scale>1</Scale>
				<X>0</X>
				<Y>0</Y>
			</PowerArmor>
		</TeamPanel>
		<FusionCoreMeter>
			<Scale>1</Scale>
			<X>0</X>
			<Y>0</Y>
		</FusionCoreMeter>
		<Subtitles>
			<Scale>1</Scale>
			<X>0</X>
			<Y>0</Y>
		</Subtitles>
		<XPBar>
			<Scale>1</Scale>
			<X>0</X>
			<Y>0</Y>
		</XPBar>
		<CritMeter>
			<Scale>1</Scale>
			<X>0</X>
			<Y>0</Y>
		</CritMeter>
		<CurrencyUpdates>
			<Scale>1</Scale>
			<X>0</X>
			<Y>0</Y>
		</CurrencyUpdates>
		<LevelUpAnimation>
			<Scale>1</Scale>
			<X>0</X>
			<Y>0</Y>
		</LevelUpAnimation>
		<ReputationUpdates>
			<Scale>1</Scale>
			<X>0</X>
			<Y>0</Y>
		</ReputationUpdates>
		<EnemyHealthMeter>
			<Scale>1</Scale>
			<X>0</X>
			<Y>0</Y>
			<!--
				Shows health percentage near the bar, a la Show Health Re-Redux
			-->
			<ShowHealthPercent>false</ShowHealthPercent>
			
			<!-- top, bottom, left, right -->
			<HealthPercentLocation>top</HealthPercentLocation>
		</EnemyHealthMeter>
        <HitMarker>
			<Scale>1</Scale>
			<X>0</X>
			<Y>0</Y>
		</HitMarker>
	</Elements>
		<!--
		ImmersiveTweaks
		
		Concept and Code Scrapping By Commanderbass.
		Active Effects, Emote Bubble, Stealth Meter, Perkpopup, Code Sanity Checking, and So Much More By Ms.Zelia 
	
		Explaination for the Standard Options Provided:
		
		Hide: 
			True: Hide the Listed Hud Element. Same as Setting Scale to 0 but Allows You to Keep Scale Settings
			False: Display as defined by other parms.
			
		VaTs:
			True: Hide the Element Unless User is Using Vats.
			False: Display as defined by other parms.
			
		Crouch: 
			True: Hide the Element Unless User is Crouched.
			False: Display as defined by other parms.
			
		Fade:
			True: Hide the Element when ABOVE the percent defined by FadePer.
				  IE if HP FadePer = 20 Your Hp Will Only Appear When You Drop to 19.9% Hp.
			False: Display as defined by other parms.
		
			
		Known Bugs:
		
		1} Sometimes When Looking at a Container the HUD will Appear for a Frame.
		
		2} Some Elements Will Flicker for a Single Frame When First Set to Hidden.
			Fixed Where I Could.
			
		3} Some Options Take a Second to Update Their Settings.
		
		For Elements Not Covered Such as Level Up Animation Try Fanfare Free by Mapex:
		Nexus Link: https://www.nexusmods.com/fallout76/mods/1293
	-->
	<ImmersiveTweaks>
		<Debug>
			<Display>false</Display>
		</Debug>
		<WaterMark>
			<!--Settings for Default HUD Editor WaterMark-->
			<HideDefault>true</HideDefault>
			<CustomWaterMark>true</CustomWaterMark>
				<CustomWaterMarkEntry>Like And Subscribe</CustomWaterMarkEntry>
			<!--Settings for HUD Editor Edit Mode WaterMark-->
			<HideEditMode>true</HideEditMode>
			<CustomEditMark>true</CustomEditMark>
				<CustomEditMarkEntry>Edit Mode Enabled</CustomEditMarkEntry>
		</WaterMark>
		<ActiveEffects>
			<Hide>false</Hide>
			<Crouch>false</Crouch>
		<!--Only Display Negative Effects IE Overweight, Withdraw, Ect. Updates When You Gain a New Effect.-->
			<OnlyNegativeEffects>false</OnlyNegativeEffects>
		</ActiveEffects>
		<Ap>
			<Hide>false</Hide>
			<Crouch>false</Crouch>
			<VaTs>false</VaTs>
			<Fade>false</Fade>
				<FadePer>25</FadePer>
		</Ap>
		<Compass>
			<Hide>false</Hide>
		<!--Weird Base Game Bug: Sometimes When Crouched You Will Lose Compass Markers for NPCs.-->
			<Crouch>false</Crouch>
		</Compass>
		<Emote>
		<!--Hides the Emote Widget on the Side of the Screen-->
			<HideEmoteWidget>false</HideEmoteWidget>
		<!--Disables All Above Head Pop-Ups. Includes: Emotes, Trade Pop-Ups, Revive Requests, Fear Emotes.-->
			<HidePlayerEmoteBubble>false</HidePlayerEmoteBubble>
		</Emote>
		<FusionCoreMeter>
			<Hide>false</Hide>
			<Crouch>false</Crouch>
			<VaTs>false</VaTs>
			<Fade>false</Fade>
				<FadePer>10</FadePer>
		</FusionCoreMeter>
		<FusionCoreWarning>
			<Hide>false</Hide>
		<!--Allows You to Edit The Text Shown When in the 2 States. Updates on State Change-->		
			<CustomText>
				<Enable>false</Enable>
				<LowBtry>Shit, Cores Getting Low</LowBtry>
				<NoBtry>Fuck, Out of Juice</NoBtry>
			</CustomText>
		<!--
		Scale Does Nothing Above Max Scale [1.5].
		-->
			<Scale>1</Scale>
			<X>0</X>
			<Y>0</Y>
		</FusionCoreWarning>
		<HungerThirst>
			<Hide>false</Hide>
			<Crouch>false</Crouch>
		</HungerThirst>
		<Hp>
			<Hide>false</Hide>
			<Crouch>false</Crouch>
			<VaTs>false</VaTs>
			<Fade>false</Fade>
				<FadePer>10</FadePer>
		</Hp>
		<!--Scale and movment options for the orginal improved healthbar mod [https://www.nexusmods.com/fallout76/mods/368]
			Only enable if you are running the mod, and if you want to edit the postion
			Only edits the HP% Counter.-->
		<ImprovedHealthBar>
			<Enable>false</Enable>
				<Scale>1.0</Scale>
				<X>-250</X>
				<Y>0</Y>
		</ImprovedHealthBar>
		<Indicators>
		<!--Hides the Arrow Pointing at Nearby Explosion Sources-->
			<HideExplosiveIndicators>false</HideExplosiveIndicators>
		<!--Hides the Arrow Indicating Damage Taken and its Direction-->
			<HideDirectionalHitIndicator>false</HideDirectionalHitIndicator>
		<!--Scale Options for the Floating Dmg Numbers. -->
			<FloatingDmgNumbScale>1.0</FloatingDmgNumbScale>
		</Indicators>
		<InteractPrompts>
		<!--Quickloot (which annoyingly) applies to some containers rollover-->
			<QuickLoot>
				<Hide>false</Hide>
				<Crouch>false</Crouch>
			</QuickLoot>
		<!--Frobber is interact prompts like unlicking locked doors and touching NPCs-->
			<Frobber>
				<Hide>false</Hide>
				<Crouch>false</Crouch>
			</Frobber>
		<!--RollOver is essentially the same as Frobber (interact prompts)-->
			<RollOver>
				<Hide>false</Hide>
				<Crouch>false</Crouch>
			</RollOver>
		</InteractPrompts>
		<PerkPopUp>
			<Hide>false</Hide>
		<!--
			Scale Does Nothing Above Max Scale [1.5].
		-->
			<Scale>1</Scale>
			<X>0</X>
			<Y>0</Y>
		</PerkPopUp>
		<RadDamageIndicator>
			<Hide>false</Hide>
				<OnlyHideSymbol>false</OnlyHideSymbol>
				<OnlyHideText>false</OnlyHideText>
			<CustomText>
				<Enable>false</Enable>
				<Text>Air is Getting Heavy</Text>
			</CustomText>
		<!--
		Scale Does Nothing Above Max Scale [1.5].
		-->
			<Scale>1</Scale>
			<X>0</X>
			<Y>0</Y>
		</RadDamageIndicator>
		<StealthMeter>
			<Hide>false</Hide>
		<!--Only Display When in Caution or Danger. Can Run With ThiefSense.-->
			<SpiderSense>false</SpiderSense>
		<!--Only Display When Detected. Can Run with SpiderSense.-->
			<ThiefSense>false</ThiefSense>
		<!--Allows You to Edit The Text Shown When in the 4 States. Updates on State Change-->	
			<CustomText>
				<Enable>false</Enable>
				<HiddenText>QUIET AS THE GRAVE</HiddenText>
				<DetectedText>FRIENDLY?</DetectedText>
				<CautionText>SHIT....</CautionText>
				<DangerText>SHIT! FUCK!</DangerText>
			</CustomText>
		<!--Disables the [] on the Stealth Meter.-->
		<DisableBrackets>false</DisableBrackets>
		</StealthMeter>
		<TeamPanel>
		<!--Any Team Options Will Also Hide the Area Chat indicator-->
			<Hide>false</Hide>
			<Crouch>false</Crouch>
		<!--Hides the Team Panel if You Are the Only Player on Team.-->
			<HideEmptyTeam>false</HideEmptyTeam>
		<!--Hides the Small Emote Icon.-->	
			<HideEmote>false</HideEmote>
		</TeamPanel>
		<Xp>
			<Hide>false</Hide>
		</Xp>
	</ImmersiveTweaks>
	<!--
		A bit of a small explaination for the options:
		RGB: Color you want in hex form (FFFFCB is the default HUD color)
		NOTE: DO NOT use very dark or very bright colors! Try to pick something pastel or less in saturation.
		Brightness, Contrast, Saturation: self explainatory. Absolute ranges are from -100 to 100, but 00 to 50 is the highly recommended min-max
	-->
	<Colors>
		<!--Tints the hitmarker towards the given colour-->
		<!--KEEP IN MIND: the marker is red by default ingame-->
		<HitMarkerTint>
			<RGB>881111</RGB>
			<Brightness>-10</Brightness>
			<Contrast>10</Contrast>
			<Saturation>10</Saturation>
		</HitMarkerTint>
		
		<HUD>
			<!--Set this to false to completely disable all HUD recoloring. Requires leaving/rejoining server to apply.-->
			<EnableRecoloring>true</EnableRecoloring>
			
			<!--Show a percentage value above the thirst and hunger bars-->
			<ThirstHungerPercentShow>false</ThirstHungerPercentShow>
			
			<!--Prevents thirst and hunger from auto-hiding after a few seconds-->
			<AlwaysShowThirstHunger>false</AlwaysShowThirstHunger>
			
			<!--EditMode allows you to view changes to this file in real time-->
			<EditMode>false</EditMode>
			
			<!--DO NOT CHANGE THIS SETTING, IT WILL BE REMOVED AT A FUTURE TIME-->
			<CustomCrosshair>true</CustomCrosshair>
			
			<!--You can set the opacity of the crosshair down below what the rest of the HUD is.-->
			<CrosshairOpacity>1</CrosshairOpacity>
			
			<!--set this to true if you use TZMapMarkers. Disables the coloring of compass markers.-->
			<TZMapMarkers>false</TZMapMarkers>
		</HUD>
		
		<!--RightMeters are things like ammo, active effects, thirst/hunger, etc-->
		<RightMeters>
			<RGB>FFFFCB</RGB>
			<Brightness>0</Brightness>
			<Contrast>0</Contrast>
			<Saturation>0</Saturation>
		</RightMeters>
		
		<!--LeftMeters is mainly health and rads-->
		<LeftMeters>
			<RGB>FFFFCB</RGB>
			<Brightness>0</Brightness>
			<Contrast>0</Contrast>
			<Saturation>0</Saturation>
			<!--remember, rads bar is light red/almost orange by default, radsbarRGB tints the bar towards your colour, might not be 100% accurate-->
			<RadsBarRGB>F67259</RadsBarRGB>
			<RadsBarBrightness>-10</RadsBarBrightness>
			<RadsBarContrast>0</RadsBarContrast>
			<RadsBarSaturation>0</RadsBarSaturation>
		</LeftMeters>
		
		<!--Notifications are like Sneak damage etc-->
		<Notifications>
			<RGB>FFFFCB</RGB>
			<Brightness>0</Brightness>
			<Contrast>0</Contrast>
			<Saturation>0</Saturation>
		</Notifications>
		
		<!--HudFrobber is the loot menu for world containers, NPC interaction prompt, doors etc-->
		<HudFrobber>
			<RGB>FFFFCB</RGB>
			<Brightness>0</Brightness>
			<Contrast>0</Contrast>
			<Saturation>0</Saturation>
		</HudFrobber>
		
		<!--QuestTracker is the upper right quest panel-->
		<QuestTracker>
			<RGB>FFFFCB</RGB>
			<Brightness>0</Brightness>
			<Contrast>0</Contrast>
			<Saturation>0</Saturation>
		</QuestTracker>
		
		<!--TopCenter is the enemy health/sneak bar-->
		<TopCenter>
			<RGB>FFFFCB</RGB>
			<Brightness>0</Brightness>
			<Contrast>0</Contrast>
			<Saturation>0</Saturation>
		</TopCenter>
		
		<!--BottomCenter is Subtitles, Compass and Crit Meter-->
		<BottomCenter>
			<RGB>FFFFCB</RGB>
			<Brightness>0</Brightness>
			<Contrast>0</Contrast>
			<Saturation>0</Saturation>
			
			<CompassRGB>FFFFCB</CompassRGB>
			<CompassBrightness>0</CompassBrightness>
			<CompassContrast>0</CompassContrast>
			<CompassSaturation>0</CompassSaturation>
		</BottomCenter>
		
		<!--Announce is the text of new events etc-->
		<Announce>
			<RGB>FFFFCB</RGB>
			<Brightness>0</Brightness>
			<Contrast>0</Contrast>
			<Saturation>0</Saturation>
		</Announce>
		
		<!--Center is crosshair, quickloot, interaction menus of things not covered in HudFrobber-->
		<Center>
			<RGB>FFFFCB</RGB>
			<Brightness>0</Brightness>
			<Contrast>0</Contrast>
			<Saturation>0</Saturation>
		</Center>
		
		<!--Team panel on HUD-->
		<Team>
			<RGB>FFFFCB</RGB>
			<Brightness>0</Brightness>
			<Contrast>0</Contrast>
			<Saturation>0</Saturation>
			<!--remember, rads bar is light red/almost orange by default, radsbarRGB tints the bar towards your colour, might not be 100% accurate-->
			<RadsBarRGB>F67259</RadsBarRGB>
			<RadsBarBrightness>-10</RadsBarBrightness>
			<RadsBarContrast>0</RadsBarContrast>
			<RadsBarSaturation>0</RadsBarSaturation>
		</Team>
		
		<!--Floating handles things like the damage numbers, floating quest markers, etc-->
		<Floating>
			<RGB>FFFFCB</RGB>
			<Brightness>0</Brightness>
			<Contrast>0</Contrast>
			<Saturation>0</Saturation>
		</Floating>
	</Colors>
</HUDEditor>

